{
  "presets": [
    "@babel/preset-env"
  ],
  "plugins": [
    "transform-object-assign",
    "@babel/plugin-proposal-optional-chaining",
    "@babel/plugin-proposal-class-properties",
    [
      "transform-react-jsx",
      {
        "pragma": "h"
      }
    ],
    [
      "jsx-pragmatic",
      {
        "module": "preact",
        "export": "h",
        "import": "h"
      }
    ]
  ]
}
